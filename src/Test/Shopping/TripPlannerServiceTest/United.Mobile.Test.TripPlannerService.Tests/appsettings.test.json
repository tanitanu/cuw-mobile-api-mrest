{
  "Globalization": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Override": {
        "Default": "Information",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "United.Ebs.Logging.Providers.LogFormatter, United.Ebs.Logging"
        }
      }
    ],
    "Properties": {
      "Application": "United.Mobile.Services.TripPlannerService.Api"
    }
  },
  "sessionConfig": {
    "baseUrl": "https://mobileapi.qa.cuw.aws.ual.com/shopsessionservice/api/",
    "timeoutPolicyConfig": {
      "seconds": 30
    },
    "retryPolicyConfig": {
      "RetryCount": 1
    },
    "circuitBreakerPolicyConfig": {
      "AllowExceptions": 1,
      "BreakDuration": 30
    }
  },
  "cachingConfig": {
    "baseUrl": "https://mobileapi.qa.cuw.aws.ual.com/cacheservice/api/",
    "timeoutPolicyConfig": {
      "seconds": 30
    },
    "retryPolicyConfig": {
      "RetryCount": 1
    },
    "circuitBreakerPolicyConfig": {
      "AllowExceptions": 1,
      "BreakDuration": 30
    }
  },
  "dpTokenConfig": {
    "baseUrl": "https://aws.qagw.api.united.com/oauth2/v1/token",
    "tokenKeyFormat": "MICROSERVICE::QA::{0}::{1}",
    "tokenExpInSec": "3600",
    "timeoutPolicyConfig": {
      "seconds": 30
    },
    "retryPolicyConfig": {
      "RetryCount": 1
    },
    "circuitBreakerPolicyConfig": {
      "AllowExceptions": 1,
      "BreakDuration": 30
    }
  },
  "DynamoDBClient": {
    "baseUrl": "https://mobileapi.qa.cuw.aws.ual.com/dynamodbservice/api",
    "timeoutPolicyConfig": {
      "seconds": 30
    },
    "retryPolicyConfig": {
      "RetryCount": 1
    },
    "circuitBreakerPolicyConfig": {
      "AllowExceptions": 1,
      "BreakDuration": 30
    }
  },
  "AddTripPlanVotingServiceClient": {
    "baseUrl": "https://aws.qagw.api.united.com/8.0/shoppingcart/travelplanner/api",
    "timeoutPolicyConfig": {
      "seconds": 30
    },
    "retryPolicyConfig": {
      "RetryCount": 1
    },
    "circuitBreakerPolicyConfig": {
      "AllowExceptions": 1,
      "BreakDuration": 30
    }
  },
  "Booking2OGenericExceptionMessage": "Sorry, something went wrong. Please try again.",
  "EnableTripPlanPushNotifications": "true",
  "IsProd": "false",
  "TripPlanPushNotificationTitle": "United Trip Planner",
  "TripPlanPushNotificationBody": "There's a new vote on your shared itinerary to ",
  "SessionExpiryMessageRedirectToHomerBookingMain": "true",
  "loadSessionFromOnPremCouchbase": true

}
